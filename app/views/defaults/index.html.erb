<div class="row">
  <div class="page-header" style="border: 0;">
    <h1>Sistema de gesti√≥n de activos</h1>
  </div>
  <div class="col-xs-12">
    <% block_per_row = 3 %>
    <% (@blocks.length / block_per_row).times do |row| %>
      <div class="row">
        <% block_per_row.times do |i| %>
            <% block = @blocks[block_per_row * row + i] %>
            <%= render 'defaults/partials/cell', url: block[:url], name: block[:name], icon: block[:icon] %>
      <% end %>
      </div>
    <% end %>
  </div>
</div>