<% title 'Agregar detalles técnicos' %>
<div class="row">
  <div class="page-header">
    <h1>Agregar detalles técnicos</h1>
  </div>
  <div class="col-xs-12 col-sm-10 col-md-8 center-block">
    <%= render 'assets/partials/card', asset: @asset %>
    <%= simple_form_for @technical_detail, url: asset_technical_details_path(@asset) do |f| %>
      <%= render 'technical_details/forms/fields', f: f %>
      <%= f.submit 'Guardar', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
